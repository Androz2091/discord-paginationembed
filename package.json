{
  "name": "discord-paginationembed",
  "version": "0.8.0",
  "description": "Pagination Embed for Discord.JS",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "scripts": {
    "test": "yarn run lint && yarn run docs:build",
    "test:bot": "node test/bot.js",
    "lint": "yarn run src:lint && yarn run types:lint",
    "src:lint": "eslint ./src",
    "src:lint-fix": "yarn run src:lint --fix",
    "types:lint": "tslint -c tslint.json src/index.d.ts",
    "types:lint-fix": "yarn run types:lint --fix",
    "docs:build": "typedoc src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gazmull/discord-paginationembed.git"
  },
  "keywords": [
    "discord",
    "discord.js",
    "pagination",
    "embed",
    "paginationembed",
    "emoji",
    "react",
    "leaderboard"
  ],
  "author": "gazmull",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gazmull/discord-paginationembed/issues"
  },
  "homepage": "https://github.com/gazmull/discord-paginationembed#readme",
  "peerDependencies": {
    "discord.js": "github:discordjs/discord.js"
  },
  "devDependencies": {
    "@types/node": "^11.11.1",
    "eslint": "^4.19.1",
    "tslint": "4.5.1",
    "tslint-config-typings": "^0.3.1",
    "typedoc": "^0.14.2",
    "typescript": "^3.3.3333"
  }
}
