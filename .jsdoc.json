{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include": ["./src", "README.md"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": ["plugins/markdown"],
    "opts": {
        "template": "./node_modules/docdash",
        "encoding": "utf8",
        "destination": "../discord-paginationembed-docs/",
        "recurse": true,
        "verbose": true,
        "sort": true
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": false,
        "default": {
            "includeDate": false
        }
    },
    "docdash": {
        "sectionOrder": ["Classes"],
        "search": true,
        "collapse": true,
        "typedefs": true,
        "sort": true
    },
    "markdown": {
        "hardwrap": true
    }
}
