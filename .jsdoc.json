{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include": ["./src", "README.md"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [],
    "opts": {
        "template": "node_modules/docdash",
        "encoding": "utf8",
        "destination": "../discord-paginationembed-docs/",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": false,
        "default": {
            "includeDate": false
        }
    },
    "docdash": {
        "sort": true
    }
}
